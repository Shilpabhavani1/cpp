// Function should return 0 is length is even else return 1
int isLengthEvenOrOdd(struct Node* head)
{
     //Code here
     int c=0;
     Node* t=head;
     while(t!=NULL)
     {
         c++;
         t=t->next;
     }
     if(c%2==0)
     {
         return 0;
     }
     else
     {
         return 1;
     }
}
